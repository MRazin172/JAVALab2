import java.util.*;
import java.util.Scanner;

class HighestFrequencyNumbers {
    private static int[] numbers;
    int mark;

    // Method to identify top K numbers with maximum occurrences
    public static List<Integer> topkFrequentNumbers (int n) {

        // Calculate frequency of each number in the array
        for (int num : numbers) {
            int max = 0, val = 0; int count=0;
            for(int i=0;i<n;i++) {
                for(int j=0;j<n;j++) {
                    if(numbers[j] == numbers[i])
                        ++count;
                }
                if(count > max) {
                    max = count;
                    val = numbers[i];
                }
            }
        }
